<?xml version="1.0" encoding="UTF-8" ?>
<PropertyList>

<path>syd_term2.ac</path>

<animation>
 <type>material</type> 
 <object-name>syd_term2</object-name>
 <object-name>t2signs</object-name>
 <condition>		
  <greater-than>		
   <property>/sim/time/sun-angle-rad</property>
   <value>1.57</value>
  </greater-than>
 </condition>
 <emission>	
  <red>0.6</red>	
  <green>0.6</green>	
  <blue>0.6</blue>
 </emission>
 <texture>yssy-nightt2.png</texture>
</animation>

<animation>
 <type>material</type> 
 <object-name>syd_term2</object-name>
 <object-name>t2signs</object-name>
 <condition>		
  <less-than-equals>		
   <property>/sim/time/sun-angle-rad</property>
   <value>1.57</value>
  </less-than-equals>
 </condition>
 <emission>	
  <red>0.4</red>	
  <green>0.4</green>	
  <blue>0.4</blue>
 </emission>
 <texture>yssy-dayt2.png</texture>
</animation>

</PropertyList>